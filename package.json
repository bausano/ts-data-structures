    "typescript",
    "data structures",
    "optional",
    "result"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/sinon": "^7.0.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.1.1",
    "nyc": "^13.1.0",
    "sinon": "^5.1.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "nyc": {
    "extension": [
      ".ts"
